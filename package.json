{
  "name": "@vevkit/saga",
  "version": "0.1.0",
  "description": "A functional, composable logging library for JavaScript/TypeScript applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
